
# Firebase Client Configuration
# Get these from your Firebase project settings > General > Your apps > Firebase SDK snippet > Config
NEXT_PUBLIC_FIREBASE_API_KEY="changeme"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="changeme"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="changeme"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="changeme"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="changeme"
NEXT_PUBLIC_FIREBASE_APP_ID="changeme"

# Firebase Server (Admin) Configuration
# Get this from your Firebase project settings > Service accounts > Generate new private key
# CRITICAL: This MUST be a single line of JSON. Copy the entire content of the downloaded file.
FIREBASE_SERVICE_ACCOUNT_KEY='changeme'

# Genkit AI Services API Keys (Optional, but highly recommended)
# Get a Gemini API key from Google AI Studio: https://aistudio.google.com/app/apikey
GEMINI_API_KEY="changeme"

# Get an API key from Hugging Face: https://huggingface.co/settings/tokens
HUGGING_FACE_API_KEY="changeme"

# Get an API key from OpenRouter: https://openrouter.ai/keys
OPENROUTER_API_KEY="changeme"

# Get an API key from Civitai: https://civitai.com/user/account
CIVITAI_API_KEY="changeme"

# Get an API key from ModelsLab: https://modelslab.com/profile
MODELSLAB_API_KEY="changeme"

# Custom Models Storage Bucket (Optional)
# The name of the Google Cloud Storage bucket where you want to sync models for your own ComfyUI/A1111 instance.
# Example: "my-charaforge-models-bucket"
MODELS_STORAGE_BUCKET="changeme"

# Development Flags
# Set this to 'true' to connect to local Firebase emulators
NEXT_PUBLIC_USE_EMULATORS=false
